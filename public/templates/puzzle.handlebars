<table>
    <tr>
        <td id='power'>{{points}}</td>
        {{#each colMinesCount}}
        <td>{{this}}</td>
        {{/each}}
    </tr>
    {{#each field}}
    <tr>
        <td>{{lookup ../rowMinesCount @index}}</td>
        {{#each this}}
        <td class='close cell'></td>
        {{/each}} </tr>
    {{/each}}
</table>